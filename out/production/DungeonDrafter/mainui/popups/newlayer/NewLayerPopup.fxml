<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="mainui.popups.newlayer.NewLayerPopup"
      prefHeight="400.0" prefWidth="600.0">

    <HBox alignment="CENTER">
        <Label text="Layer Name:"/>
        <TextField text="New Layer" fx:id="layerNameField"/>
        <Button text="OK" onAction="#createNewLayer" fx:id="okButton"/>
    </HBox>

    <ComboBox fx:id="textureCombo">

    </ComboBox>

    <stylesheets>
        <URL value="@NewLayerPopup.css" />
    </stylesheets>

</VBox>
